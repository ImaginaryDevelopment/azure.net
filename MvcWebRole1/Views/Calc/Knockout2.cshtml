@{
    ViewBag.Title = "Knockout with Preparser";
}
<div data-ko-event="{keypress: checkkey}">
<h2>Knockout</h2>
<div data-bind="event:{keypress: checkkey}">
    <h1>@ViewBag.Title</h1>

<input type="text" data-ko-value="userinput" data-ko-valueUpdate="'afterkeydown'"
/>
<div data-ko-foreach="operations">
    <input type="button" class="operation" data-ko-attr="{title:name}" data-ko-click="fn" data-ko-value="key" />
</div>
<div class="result" data-ko-text="result"></div>
<div class=" age " data-ko-text="loaded"></div>
</div>
</div>
<a class="jsfiddle" href="http://jsfiddle.net/Maslow/hsHt2/">Knockout</a>
@section scripts
{
    @Scripts.Render(Links.Scripts.knockout_2_2_0_js)    
    @Scripts.Render(Links.Scripts.knockout_preparser_js)
    @Scripts.Render(Links.Scripts.Calc.calc_knockout_js)
}
